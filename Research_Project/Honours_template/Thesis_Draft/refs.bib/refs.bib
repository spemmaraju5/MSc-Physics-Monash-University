	keywords = {High Energy Physics - Experiment, High Energy Physics - Phenomenology},
	pages = {111802},
	annote = {Comment: 7 pages, 4 figures. Appendix A added to show sensitivity to scenario with additional hypercharge couplings; references added; additional bound on K decays from E949 added (results qualitatively unchanged). Version to be published in PRL},
	file = {arXiv Fulltext PDF:files/27/Izaguirre et al. - 2017 - A New Flavor of Searches for Axion-Like Particles.pdf:application/pdf;arXiv.org Snapshot:files/28/1611.html:text/html},

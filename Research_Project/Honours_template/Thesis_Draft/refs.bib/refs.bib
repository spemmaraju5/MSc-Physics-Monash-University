@article{izaguirre_new_2017,
	title = {A {New} {Flavor} of {Searches} for {Axion}-{Like} {Particles}},
	volume = {118},
	issn = {0031-9007, 1079-7114},
	url = {http://arxiv.org/abs/1611.09355},
	doi = {10.1103/PhysRevLett.118.111802},
	abstract = {We propose new searches for axion-like particles (ALPs) produced in flavor-changing neutral current (FCNC) processes. This proposal exploits the often-overlooked coupling of ALPs to W bosons, leading to FCNC production of ALPs even in the absence of a direct coupling to fermions. Our proposed searches for resonant ALP production in decays such as \$B{\textbackslash}rightarrow K{\textasciicircum}\{(*)\}a,{\textbackslash},a{\textbackslash}rightarrow {\textbackslash}gamma{\textbackslash}gamma\$ and \$K{\textbackslash}rightarrow {\textbackslash}pi a, {\textbackslash},a{\textbackslash}rightarrow {\textbackslash}gamma{\textbackslash}gamma\$ could greatly improve upon the current sensitivity to ALP couplings to Standard Model particles. We also determine analogous constraints and discovery prospects for invisibly decaying ALPs.},
	number = {11},
	urldate = {2022-06-06},
	journal = {Physical Review Letters},
	author = {Izaguirre, Eder and Lin, Tongyan and Shuve, Brian},
	month = mar,
	year = {2017},

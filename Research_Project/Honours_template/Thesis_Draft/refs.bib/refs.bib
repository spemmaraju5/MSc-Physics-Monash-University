@article{lhcb_collaboration_measurement_2013,
	title = {Measurement of the ratio of branching fractions {BR}({B0} -{\textgreater} {K}*0 gamma)/{BR}({Bs0} -{\textgreater} phi gamma) and the direct {CP} asymmetry in {B0} -{\textgreater} {K}*0 gamma},
	volume = {867},
	issn = {05503213},
	url = {http://arxiv.org/abs/1209.0313},
	doi = {10.1016/j.nuclphysb.2012.09.013},
	abstract = {The ratio of branching fractions of the radiative B decays B0 -{\textgreater} K*0 gamma and Bs0 phi gamma has been measured using an integrated luminosity of 1.0 fb-1 of pp collision data collected by the LHCb experiment at a centre-of-mass energy of sqrt(s)=7 TeV. The value obtained is BR(B0 -{\textgreater} K*0 gamma)/BR(Bs0 -{\textgreater} phi gamma) = 1.23 +/- 0.06(stat.) +/- 0.04(syst.) +/- 0.10(fs/fd), where the first uncertainty is statistical, the second is the experimental systematic uncertainty and the third is associated with the ratio of fragmentation fractions fs/fd. Using the world average value for BR(B0 -{\textgreater} K*0 gamma), the branching fraction BR(Bs0 -{\textgreater} phi gamma) is measured to be (3.5 +/- 0.4) x 10{\textasciicircum}\{-5\}. The direct CP asymmetry in B0 -{\textgreater} K*0 gamma decays has also been measured with the same data and found to be A(CP)(B0 -{\textgreater} K*0 gamma) = (0.8 +/- 1.7(stat.) +/- 0.9(syst.))\%. Both measurements are the most precise to date and are in agreement with the previous experimental results and theoretical expectations.},
